<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane
        alignment="center"
        hgap="10" vgap="10"
        xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal"
        fx:controller="sample.Controller"
        id="grid_pane">
    <children>
        <Text fx:id="map" />
        <ButtonBar GridPane.rowIndex="3">
            <buttons>
                <Button text="Adauga rind" onAction="#handleAddRowButtonAction" />
                <Button text="Sterge rind" />
                <Button text="Adauga coloana" />
                <Button text="Sterge coloana" />
            </buttons>
        </ButtonBar>
<!--        <DialogPane visible="false">Selecteaza o optiune pentru a continua</DialogPane>-->
    </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
